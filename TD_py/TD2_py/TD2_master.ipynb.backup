        "    sigma=0.3\n",
        "    p=1.0/2.0;u=1.0-sigma/math.sqrt(N);d=1.0+sigma/math.sqrt(N)\n",
        "    x_0=100\n",
        "\n",
        "    X=simul_cox_ross(N,x_0,p,u,d)\n",
        "    plt.plot(X)"
